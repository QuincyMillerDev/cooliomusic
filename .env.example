# ElevenLabs API Key
# Get yours at: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Stability API KEY
STABILITY_API_KEY=your_api_key_here

# OpenRouter API Key (for AI agent)
# Get yours at: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Override default AI model
# OPENROUTER_MODEL=anthropic/claude-sonnet-4-20250514

# Optional: Override output directory
# OUTPUT_DIR=output/audio

# Cloudflare R2 Storage (for track library)
# Create an API token at: Cloudflare Dashboard → R2 → Manage R2 API Tokens
R2_ACCESS_KEY_ID=your_r2_access_key_id_here
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key_here
R2_BUCKET_NAME=cooliomusicstorage
R2_ENDPOINT_URL=https://ccbe407ceb8cc78fc1ec28cbb02894b0.r2.cloudflarestorage.com

FAL_KEY="your_fal_key"           # from https://fal.ai/dashboard/keys
RUNWAY_API_SECRET="your_runway_key"  # from https://app.runwayml.com/api-tokens

KLING_AI_ACCESS_KEY=keyhere
KLING_AI_SECRET_KEY=keyhere